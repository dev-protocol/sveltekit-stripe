{
	"name": "sveltekit-stripe",
	"version": "1.0.2",
	"description": "SvelteKit components for concisely rendering Stripe UI elements",
	"repository": {
      "type": "git",
      "url": "https://github.com/pevey/sveltekit-stripe"
   },
	"homepage": "https://pevey.com/sveltekit-stripe",
	"author": "Lacey Pevey",
	"license": "MIT",
	"keywords": [
	  "svelte",
	  "sveltekit",
	  "stripe",
	  "payment",
	  "payments",
	  "address",
	  "elements",
	  "pricing table",
	  "checkout",
	  "subscriptions",
	  "ecommerce"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./Address.svelte": {
			"types": "./dist/Address.svelte.d.ts",
			"svelte": "./dist/Address.svelte"
		},
		"./Payment.svelte": {
			"types": "./dist/Payment.svelte.d.ts",
			"svelte": "./dist/Payment.svelte"
		},
		"./PricingTable.svelte": {
			"types": "./dist/PricingTable.svelte.d.ts",
			"svelte": "./dist/PricingTable.svelte"
		},
		"./stores.js": {
			"types": "./dist/stores.d.ts",
			"svelte": "./dist/stores.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.0",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@stripe/stripe-js": "^1.52.1"
	}
}
